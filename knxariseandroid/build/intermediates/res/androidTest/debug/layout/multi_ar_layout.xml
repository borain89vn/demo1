<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/multiARLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <ImageView
        android:id="@+id/blur_camera_frame"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:contentDescription="@string/imageView" />
    
    <ImageView
        android:id="@+id/multi_ar_dim_background"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#99000000"
        android:contentDescription="@string/imageView" />

    <TextView
        android:id="@+id/arHeader"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="top"
        android:gravity="center"
        android:background="@color/default_theme_color"
        android:text="@string/txtARListTitle"
        android:textColor="@color/default_text_color"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textAllCaps="true" />
    
    <ListView
        android:id="@+id/listview" 
        android:layout_below="@+id/arHeader"
        android:background="@android:color/transparent"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:divider="@null"
        android:dividerHeight="0dp" >
        
    </ListView>
    
    <Button
        android:id="@+id/numerItem"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:background="@color/default_theme_color"
        android:gravity="center"
        android:text=""
        android:textColor="@color/default_text_color"
        android:textStyle="italic" />

</RelativeLayout><!-- From: file:/E:/ThanhNiewNewsXuan-AS/knxariseandroid/src/main/res/layout/multi_ar_layout.xml --><!-- From: file:/E:/ThanhNiewNewsXuan-AS/knxariseandroid/build/intermediates/bundles/debug/res/layout/multi_ar_layout.xml -->