<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/productCardLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <View
        android:id="@+id/productCardDimBackground"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#B3000000"
        android:layout_centerInParent="true" />
    
    <RelativeLayout
        android:id="@+id/productCard"
        android:layout_width="300dp"
        android:layout_height="400dp"
        android:layout_centerInParent="true"
        android:background="#FFFFFF" >
        
        <ImageView
            android:id="@+id/productPreviewImage"
            android:layout_width="100dp"
        	android:layout_height="100dp"
        	android:layout_marginTop="20dp"
        	android:layout_marginLeft="20dp"
        	android:contentDescription="@string/imageView" >
        </ImageView>
	    
    </RelativeLayout>
    
</RelativeLayout><!-- From: file:/E:/ThanhNiewNewsXuan-AS/knxariseandroid/src/main/res/layout/product_card_layout.xml --><!-- From: file:/E:/ThanhNiewNewsXuan-AS/thanhNienNews/build/intermediates/exploded-aar/ThanhNiewNewsXuan-AS/knxariseandroid/unspecified/res/layout/product_card_layout.xml -->